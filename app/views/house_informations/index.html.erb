<p id="notice"><%= notice %></p>

<h1>一覧</h1>

<table>
  <thead>
    <tr>
      <th>物件名</th>
      <th>賃料</th>
      <th>住所</th>
      <th>築年数</th>
      <th>備考</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @house_informations.each do |house_information| %>
      <tr id="property">
        <td><%= house_information.property_name %></td>
        <td id="house_rent"><%= house_information.house_rent %>円</td>
        <td id="adress"><%= house_information.adsress %></td>
        <td><%= house_information.age_of_a_build %>年</td>
        <td id="remark_column"><%= house_information.remark_column %></td>
        <td><%= link_to '詳細', house_information %></td>
        <td><%= link_to '編集', edit_house_information_path(house_information) %></td>
        <td><%= link_to '削除', house_information, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to '物件登録', new_house_information_path %>
